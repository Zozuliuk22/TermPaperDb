@model TermPaperDb.Models.RouteEditModel
@{
    ViewBag.Title = "Edit";
}
<br />
<br />


<h3>Відредагувати існуючий маршрут</h3>
<br />
<br />
@using (Html.BeginForm("Edit", "Route"))
{
    <h5>
        @Html.LabelFor(m => m.Name)
    </h5>
    <br />
    @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
    <br />
    <br />
    @Html.HiddenFor(m => m.Id)
    <button type="submit" class="btn btn-success">Edit (Редагувати)</button>
}
<br />
@Html.ActionLink("Cancel (Відмінити)", "Index", "Route", new { @class = "btn btn-info" })
